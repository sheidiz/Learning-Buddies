spring.application.name=learning-buddies-backend

spring.jpa.hibernate.ddl-auto=update
spring.datasource.url=jdbc:mysql://localhost:3306/learning_buddies?useSSL=false&createDatabaseIfNotExist=true
spring.datasource.username=root
spring.datasource.password=${MYSQL_PASSWORD}
spring.mvc.hiddenmethod.filter.enabled=true